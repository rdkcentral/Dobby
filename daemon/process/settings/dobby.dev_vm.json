{
    "paths": {
      "workspaceDir": "/tmp/rdk",
      "persistentDir": "/opt/persistent/rdk"
    },

    "logging": {
      "consoleSocket": "/tmp/dobbyPty.sock"
    },

    "extraEnvVariables": [
      "ETHAN_STB_TYPE=IP",
      "ETHAN_STB_MODEL=VM"
    ],

    "network": {
      "externalInterfaces": [ "enp0s8", "enp0s3", "docker0", "eth0" ],
      "addressRange": "100.64.11.0"
    },

    "logRelay": {
      "syslog": {
        "enable": true,
        "socketPath": "/tmp/dobby-syslog"
      },
      "journald": {
        "enable": true,
        "socketPath": "/tmp/dobby-journald"
      }
    }
  }



